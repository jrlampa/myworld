{
  "name": "sisrua-unified",
  "private": true,
  "version": "1.0.0",
  "description": "Sistema de Exporta√ß√£o OSM para DXF - IMPORTANT: Do not update version manually. Use ./scripts/update-version.sh instead. See VERSIONING.md for details.",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\"",
    "client": "vite",
    "build": "tsc && vite build",
    "build:all": "powershell -ExecutionPolicy Bypass -File scripts/build_release.ps1",
    "preview": "vite preview",
    "server": "tsx watch server/index.ts",
    "version:update": "bash scripts/update-version.sh",
    "version:check": "echo 'VERSION file:' && cat VERSION && echo '' && echo 'package.json:' && grep '\"version\"' package.json | head -1 && echo '' && echo 'constants.py:' && grep 'PROJECT_VERSION' py_engine/constants.py && echo '' && echo 'useFileOperations.ts:' && grep 'PROJECT_VERSION' src/hooks/useFileOperations.ts | head -1",
    "docker:build": "docker build -t sisrua-unified:latest .",
    "docker:run": "docker run -p 8080:8080 --env-file .env sisrua-unified:latest",
    "docker:dev": "docker compose up",
    "docker:dev:build": "docker compose up --build",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f app",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "vitest run --coverage",
    "test:backend": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:backend && npm run test:frontend && npm run test:e2e",
    "security:audit": "npm audit --audit-level=moderate",
    "security:audit:fix": "npm audit fix",
    "security:check": "npm run security:audit && echo '\n--- Security audit complete ---'",
    "postinstall": "echo '\n‚úÖ Setup complete!\n\nüê≥ RECOMMENDED: Use Docker for the best experience:\n   npm run docker:dev\n\nüíª Or install Python dependencies manually:\n   pip install -r py_engine/requirements.txt\n'"
  },
  "dependencies": {
    "@google-cloud/tasks": "^6.2.1",
    "@types/multer": "^2.0.0",
    "@types/proj4": "^2.5.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.3.1",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^12.34.0",
    "groq-sdk": "^0.37.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.564.0",
    "multer": "^2.0.2",
    "proj4": "^2.20.2",
    "python-shell": "^5.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-leaflet": "^5.0.0",
    "recharts": "^3.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/geojson": "^7946.0.16",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^22.19.11",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^1.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "jsdom": "^28.1.0",
    "postcss": "^8.4.35",
    "supertest": "^7.2.2",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^1.3.1",
    "wait-on": "^9.0.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/server/tests/**/*.test.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/server/tests/setup.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "coverageDirectory": "./coverage/backend"
  }
}
