{
  "name": "sisrua-unified",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"vite\"",
    "client": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "tsx watch server/index.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "vitest run --coverage",
    "test:backend": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:backend && npm run test:frontend && npm run test:e2e",
    "postinstall": "echo 'Setup complete. Please ensure Python dependencies are installed using pip install -r py_engine/requirements.txt'"
  },
  "dependencies": {
    "@types/multer": "^2.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.3.1",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^12.34.0",
    "groq-sdk": "^0.37.0",
    "ioredis": "^5.9.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.564.0",
    "multer": "^2.0.2",
    "python-shell": "^5.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-leaflet": "^5.0.0",
    "recharts": "^3.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/bull": "^4.10.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/geojson": "^7946.0.16",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^22.19.11",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^1.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "jsdom": "^28.1.0",
    "postcss": "^8.4.35",
    "supertest": "^7.2.2",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^1.3.1",
    "wait-on": "^9.0.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/server/tests/**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "coverageDirectory": "./coverage/backend"
  }
}
